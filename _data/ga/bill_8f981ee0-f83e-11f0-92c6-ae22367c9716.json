{"legislative_session": "2025_26", "identifier": "HB 995", "title": "Vape-Free Schools Act; enact", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "description", "abstract": "A BILL to be entitled an Act to amend Part 3 of Article 16 of Chapter 2 of Title 20 of the Official Code of Georgia Annotated, relating to student health in elementary and secondary education, so as to provide for the establishment of and purpose for the Vape-Free Schools Grant Program; to provide for the allocation of grant awards under such program; to require public schools with grades nine through 12 to acquire and install vaping detectors and include vaping policy provisions in the student code of conduct; to provide for contingent effectiveness and automatic repeal; to provide for definitions; to provide a short title; to provide for related matters; to repeal conflicting laws; and for other purposes."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "House Hopper", "date": "2026-01-15", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": []}, {"description": "House First Readers", "date": "2026-01-16", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Imani Barnes", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Imani Barnes\"}", "organization_id": null}, {"name": "Omari Crawford", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Omari Crawford\"}", "organization_id": null}, {"name": "Bryce Berry", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Bryce Berry\"}", "organization_id": null}, {"name": "Mekyah McQueen", "classification": "primary", "entity_type": "person", "primary": true, "pers